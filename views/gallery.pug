doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    title Kalyan - Gallery
    link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/css/bootstrap.min.css', rel='stylesheet', integrity='sha384-aFq/bzH65dt+w6FI2ooMVUpc+21e0SRygnTpmBvdBgSdnuTN7QbdgL+OapgHtvPp', crossorigin='anonymous')
  body(style='background-color: lavender;')
    // navigation bar
    nav.navbar.navbar-expand-lg.navbar-dark.bg-dark
      .container-fluid
        a.navbar-brand(href='/') Kalyan
        button.navbar-toggler(type='button', data-bs-toggle='collapse', data-bs-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
          span.navbar-toggler-icon
        #navbarSupportedContent.collapse.navbar-collapse
          ul.navbar-nav.me-auto.mb-2.mb-lg-0
            li.nav-item
              a.nav-link.active(aria-current='page', href='/') Home
            li.nav-item
              a.nav-link.active(href='/gallery') Gallery
            li.nav-item.dropdown
              a.nav-link.dropdown-toggle(href='#', role='button', data-bs-toggle='dropdown', aria-expanded='false')
                | Booking
              ul.dropdown-menu
                li
                  a.dropdown-item(href='/wedding')
                    | Wedding
                    | Ceremony
                li
                  a.dropdown-item(href='/engagement')
                    | Engagement
                    | Ceremony
                li
                  a.dropdown-item(href='/familyfunc') Family Function
                //
                  <li>
                  <hr class="dropdown-divider">
                  </li>
                // <li><a class="dropdown-item" href="#">Something else here</a></li>
            li.nav-item
              a.nav-link(href='/') About Us
          form.d-flex(role='search')
            input.form-control.me-2(type='search', placeholder='Search', aria-label='Search')
            button.btn.btn-outline-success(type='submit') Search
    // Gallery
    .container.my-4
      .lightbox
        .row
          .col-lg-6.mb-4
            img.w-100.mb-2.mb-md-4.shadow-1-strong.rounded(src='', data-mdb-img='https://mdbcdn.b-cdn.net/img/Photos/Slides/1.webp', alt='1')
